{
  "name": "fpkit-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "publish": "lerna publish patch --no-private --conventional-commits -m \"chore(next): publish release \"",
    "publish:next": "lerna publish patch --no-private --conventional-commits -m \"chore(next): publish next release \"",
    "publish:minor": "lerna publish minor --no-private --conventional-commits -m \"chore(next): publish release \"",
    "publish:release": "lerna publish major --no-private --conventional-commits -m \"chore(next): publish release \"",
    "oc": "oc"
  },
  "dependencies": {
    "@shawnsandy/first-paint": "^2.18.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-coverage": "^0.0.4",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "7.0.0-rc.6",
    "@storybook/addon-interactions": "7.0.0-rc.6",
    "@storybook/addon-jest": "^6.5.16",
    "@storybook/addon-links": "7.0.0-rc.6",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/blocks": "7.0.0-rc.6",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "7.0.0-rc.6",
    "@storybook/react-vite": "7.0.0-rc.6",
    "@storybook/test-runner": "^0.8.0",
    "@storybook/testing-library": "0.0.14-next.1",
    "@storybook/testing-react": "^1.3.0",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vitejs/plugin-react": "^3.1.0",
    "lerna": "^5.6.2",
    "prop-types": "^15.8.1",
    "storybook": "^7.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.4.11"
  },
  "packages": [
    "packages/*",
    "apps/*"
  ]
}